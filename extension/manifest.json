{
  "name": "hashtagdislike",
  "manifest_version": 2,
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["extension.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "webNavigation"
  ]
}
